{
  "name": "@wrtnio/openai-function-schema",
  "version": "0.0.2",
  "description": "OpenAI LLM function schema from OpenAPI (Swagger) document",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "scripts": {
    "prepare": "ts-patch install",
    "build": "npm run build:main && npm run build:test",
    "build:main": "rimraf lib && tsc && rollup -c",
    "build:test": "rimraf bin && tsc -p test/tsconfig.json",
    "dev": "npm run build:test -- --watch",
    "test": "node bin/test"
  },
  "keywords": [
    "openai",
    "llm",
    "function",
    "call",
    "calling",
    "schema",
    "openapi",
    "swagger",
    "ai",
    "chat",
    "gpt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestia/fetcher": "^3.4.1",
    "@samchon/openapi": "^0.3.0"
  },
  "devDependencies": {
    "@nestia/core": "^3.4.1",
    "@nestia/e2e": "^0.6.0",
    "@nestia/sdk": "^3.4.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "nestia": "^5.3.1",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "ts-patch": "^3.2.1",
    "typescript": "5.5.2",
    "typescript-transform-paths": "^3.4.7",
    "typia": "^6.4.0",
    "uuid": "^10.0.0"
  },
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ]
}